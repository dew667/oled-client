{
  "productName": "oled-client",
  "version": "0.0.2",
  "identifier": "com.tauri.build",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:14200",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "oled-client",
        "width": 600,
        "height": 800,  
        "decorations": false,
        "fullscreen": true,
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
      
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
